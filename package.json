{
  "name": "web3-template",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "pkg/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "turbo run postinstall",
    "send-eth": "cast send --private-key 0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80 --value 1ether"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": { "@web3modal/wagmi": "^3.3.2", "bootstrap": "^5.3.2", "react-bootstrap": "^2.9.1", "viem": "^1.19.3", "wagmi": "^1.4.7" }
}
